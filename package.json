{
  "name": "j5e",
  "version": "0.0.8",
  "description": "j5e is a device framework built for ECMA TC-53's IO pattern",
  "main": "index.js",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --harmony --require esm",
    "docs": "./node_modules/jsdoc/jsdoc.js -t ./node_modules/foodoc/template -u ./build/docs/tutorials -c ./build/docs/jsdoc.json -d docs",
    "dist": "npm run dist-moddable",
    "dist-moddable": "node ./build/dist-moddable.cjs --experimental-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtex/j5e.git"
  },
  "keywords": [
    "javascript",
    "robotics",
    "IoT",
    "embedded",
    "moddable",
    "xs",
    "esp8266",
    "led",
    "switch"
  ],
  "author": "Donovan Buck <donovan@donovan.bz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dtex/j5e/issues"
  },
  "homepage": "https://dtex.github.com/j5e",
  "devDependencies": {
    "@dtex/mock-io": "^0.0.2",
    "foodoc": "dtex/foodoc#master",
    "fs-extra": "^8.1.0",
    "jsdoc": "^3.6.3",
    "lerna": "^3.20.2"
  },
  "dependencies": {}
}
